					"    }",
					"  };",
					"",
					"function getXSRFToken(){",
					"pm.sendRequest(XSRFTokenRequest, (err, response) => {",
					"        if (err === null) {",
					"            pm.collectionVariables.set(\"Token\",response.headers.get(\"XSRF-TOKEN\"));",
					"        }",
					"    });",
					"}",
					"",
					"var getToken = true;",
					"",
					"if (!pm.collectionVariables.get('Token')) {",
